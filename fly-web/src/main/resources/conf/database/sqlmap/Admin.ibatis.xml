<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" ><sqlMap><resultMap class="com.fly.biz.common.model.user.AdminDO" id="AdminDO"><result column="admin_id" property="adminId" /><result column="admin_name" property="adminName" /><result column="admin_password" property="adminPassword" /><result column="admin_email" property="adminEmail" /><result column="attribute" property="attribute" /></resultMap>    <!-- 根据管理员名获得管理员对象 -->   <select id="AdminDAO.getAdminByName" resultMap="AdminDO">     select * from admin where admin_name=#adminName#  </select>     <!-- 根据id获得管理员对象 -->    <select id="AdminDAO.getAdminById" resultMap="AdminDO">     select * from admin where admin_id=#adminId#  </select>      <!-- 新增管理员对象 -->   <insert id="AdminDAO.insertAdmin" parameterClass="AdminDO">      insert into admin (admin_id,admin_name,admin_email) values (#adminId#,#adminName#,#adminEmail#,#adminPassword#)   </insert>      <!-- 删除管理员对象 -->   <delete id="AdminDAO.deleteAdmin">     delete from admin where admin_id=#adminId#   </delete>      <!-- 更新管理员对象 -->   <delete id="AdminDAO.updateAdmin" parameterClass="AdminDO">      update admin set admin_name=#adminName#,admin_password=#adminPassword# where admin_id=#adminId#   </delete></sqlMap>