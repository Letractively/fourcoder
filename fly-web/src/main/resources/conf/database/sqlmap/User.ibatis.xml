<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" ><sqlMap><resultMap class="com.fly.biz.common.model.user.UserDO" id="UserDOMap"><result column="user_id" property="userId" /><result column="user_name" property="userName" /><result column="user_password" property="userPassword" /><result column="create_ip" property="createIp" /><result column="last_login_ip" property="lastLoginIp" /><result column="email_address" property="emailAddress" /><result column="login_num" property="loginNum" /><result column="attribute" property="attribute" /><result column="birthday" property="birthday" /></resultMap> 	<!-- 获得用户对象 -->   <select id="UserDAO.getAllItems" resultMap="UserDOMap">    select * from user   </select>     <!-- 根据用户名获得用户对象 -->   <select id="UserDAO.getUserByName" resultMap="UserDOMap">    select * from user where user_name=#value#   </select>     <!-- 根据id获得用户对象 -->    <select id="UserDAO.getUserById" resultMap="UserDOMap">     select * from user where user_id=#value#  </select>      <!-- 新增用户对象 -->   <insert id="UserDAO.insertUser" parameterClass="UserDOMap">	insert into user 		(user_id,user_name,user_password,create_ip,last_login_ip,email_address,login_num,attribute,birthday) 	values 		(#userId#,#userName#,#userPassword#,#createIp#,#lastLoginIp#,#emailAddress#,#loginNum#,#attribute#,#birthday#)   </insert>      <!-- 删除用户对象 -->   <delete id="UserDAO.deleteUser">    delete from 		user 	where user_id=#value#   </delete>      <!-- 更新用户对象 -->   <delete id="UserDAO.updateUser" parameterClass="UserDOMap">    update user set 		user_name=#userName#,		user_password=#userPassword#,		last_login_ip=#lastLoginIp#,		email_address=#emailAddress#,		attribute= #attribute#,		birthday=#birthday#	where user_id=#userId#   </delete></sqlMap>