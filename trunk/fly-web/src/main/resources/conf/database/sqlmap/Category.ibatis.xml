<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" ><sqlMap><resultMap class="com.fly.biz.common.model.category.CategoryDO" id="CategoryDO"><result column="cat_id" property="catId" /><result column="cat_name" property="catName" /><result column="attribute" property="attribute" /></resultMap>    <!-- 根据类目名获得类目对象 -->   <select id="CategoryDAO.getCategoryByName" resultMap="CategoryDO">	<![CDATA[     select * from category where cat_name=#categoryName#	 ]]>  </select>     <!-- 根据id获得类目对象 -->    <select id="CategoryDAO.getCategoryById" resultMap="CategoryDO">	<![CDATA[     select * from category where cat_id=#categoryId#	 ]]>  </select>      <!-- 新增类目对象 -->   <insert id="CategoryDAO.insertCategory" >	<![CDATA[      insert into category (cat_id,cat_name,attribute) values (#catId#,#catName#,#attribute#)	]]>   </insert>      <!-- 删除类目对象 -->   <delete id="CategoryDAO.deleteCategory">   <![CDATA[     delete from category where cat_id=#categoryId#	]]>   </delete>      <!-- 更新类目对象 -->   <delete id="CategoryDAO.updateCategory" >    <![CDATA[      update category set cat_name=#name# where cat_id=#id#	]]>   </delete></sqlMap>