<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.velocity.toolboxlocation" value="WEB-INF/toolbox.xml" />
    <constant name="struts.velocity.configfile" value="WEB-INF/velocity.properties" /> 
	<package name="default" extends="struts-default">
		<action name="Login" class="com.fly.action.login.LoginAction">
			<result name="error">/error.html</result>
			<result name="success">/testVm.vm</result>			
		</action>
		<action name="HelloWorld" class="com.fly.test.HelloWorld">  
        	<result name="SUCCESS">/success.vm</result>  
    	</action> 
	</package>
</struts>